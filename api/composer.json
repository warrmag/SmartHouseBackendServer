{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ramsey/uuid": "^4.1",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/console": "^5.1",
        "symfony/dotenv": "^5.1",
        "symfony/error-handler": "^5.1",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^5.1",
        "symfony/messenger": "^5.1",
        "symfony/process": "^5.1",
        "symfony/security-bundle": "^5.1",
        "symfony/validator": "^5.1",
        "symfony/yaml": "^5.1"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.1",
        "symfony/profiler-pack": "^v1.0.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "ext-mongo": "1.6.16"
        }
    },
    "autoload": {
        "psr-4": {
            "Application\\": "src/Application",
            "Domain\\": "src/Domain",
            "Infrastructure\\": "src/Infrastructure",
            "UI\\": "src/UI",
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^5.1"
        }
    }
}
